{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next",
        "config": {
          "outputDirectory": "frontend/out"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/out/index.html",
        "methods": ["GET"]
      },
      {
        "src": "/api/(.*)",
        "dest": "/backend/server.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ]
  }